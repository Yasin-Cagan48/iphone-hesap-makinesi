@charset "utf-8";

body { margin: 0px; background: black; font-family: -apple-system, BlinkMacSystemFont, sans-serif; display: flex; justify-content: center; align-items: center; height: 100vh; }

.calculator { width: 320px; background: black; border-radius: 20px; padding: 20px; box-sizing: border-box; }

.display { color: white; font-size: 48px; text-align: right; margin-bottom: 20px; padding: 10px; overflow: hidden; }

.buttons { display: grid; grid-template-columns: repeat(4, 1fr); gap: 10px; }

button { height: 60px; border: none; border-radius: 50%; font-size: 24px; font-weight: 500; cursor: pointer; }

.gray { background: rgb(165, 165, 165); color: black; }

.orange { background: rgb(255, 159, 10); color: white; }

.dark { background: rgb(51, 51, 51); color: white; }

.zero { grid-column: span 2; border-radius: 30px; }
